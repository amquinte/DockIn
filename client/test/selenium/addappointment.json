    {
      "type": "verifyTextPresent",
      "text": "(123) 456-7890"
    },
    {
      "type": "verifyTextPresent",
      "text": "06/03/2017"
    },
    {
      "type": "verifyTextPresent",
      "text": "12:02PM"
    }
