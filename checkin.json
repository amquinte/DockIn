{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://emissary-2.herokuapp.com/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "LOGIN"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "username"
      },
      "text": "cse112@anchorbuddies.com"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "cse112"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "loginButton"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='row']/div[2]/ul/div/i"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Check-In"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "p"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "visitor-first"
      },
      "text": "Anchor"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "visitor-last"
      },
      "text": "Buddy"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "visitor-number"
      },
      "text": "(123) 456-7890"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input.submit-check-in"
      }
    },
    {
      "type": "goBack"
    },
    {
      "type": "goBack"
    },
    {
      "type": "verifyTextPresent",
      "text": "Anchor"
    },
    {
      "type": "verifyTextPresent",
      "text": "First: Anchor Last: Buddy Appointment Time: None Check-In Time: 6:50PM Phone Number: (123) 456-7890"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//div[@class='modal-footer']//button[.='Cancel']"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Hello, Anchor Buddies Check-In"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}